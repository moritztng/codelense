# GraphQL schema example
#
# https://gqlgen.com/getting-started/

scalar Time

type TimePoint {
	Time: Int!
	Values: [Value]!
}

type Value {
	Name: String!
	Value: Int!
}

type Organization {
	Login: String!
	AvatarUrl: String!
}

type Query {
  timePoints(fromDate: Int!, toDate: Int!, location: String!): [TimePoint!]!
  organization(githubId: Int!): Organization!
}
