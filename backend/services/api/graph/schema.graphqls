# GraphQL schema example
#
# https://gqlgen.com/getting-started/

type Organization {
  key: ID!
  login: String!
  name: String!
  created: String!
}

type Event {
  key: ID!
  type: String!
  payload: String!
  created: String!
}

type Query {
  organizations(first: Int!): [Organization]!
  events(first: Int!): [Event]!
}
